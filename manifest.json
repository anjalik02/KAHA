{
    "manifest_version": 3,
    "name": "Lyrics to Braille Conversion",
    "version": "1.0",
    "description": "Converts lyrics to braille file",
    "action": {
      "default_title": "KAHA",
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": [
          "https://genius.com/*",
          "http://genius.com/*"
        ],
        "js": [
          "script.js"
        ]
      }
    ],  
    "host_permissions": [
      "https://genius.com/*"
    ]
  }