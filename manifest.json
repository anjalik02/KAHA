{
"manifest_version": 3,
"name": "KAHA Music",
"version": "1.0",
"description": "Converts currently playing song on Spotify to Braille sheet music.",
"action": {
    "default_popup": "index.html",
    "default_title": "KAHA Music"
},
"host_permissions": [
    "http://*/*",
    "https://*/*"
],
"permissions": [
    "identity",
    "tabs",
    "scripting",
    "storage"
  ],
"background": {
    "service_worker": "background.js",
    "service_worker_scope": "/"
}
}